{
  "name": "empty-project-template",
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-navigation-redux-helpers|@react-navigation)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "e2e"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "testNamePattern": "^((?!:android:).)*$"
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "prestorybook": "rnstl",
    "storybook": "storybook start",
    "test": "jest --watchAll",
    "e2e": "detox test --configuration ios.sim",
    "e2e:dev": "detox test --configuration ios.sim --reuse"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components",
        "./storybook"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@storybook/addon-ondevice-knobs": "^4.1.3",
    "apollo-boost": "^0.1.28",
    "apollo-link-state": "^0.4.2",
    "expo": "^31.0.2",
    "graphql": "^14.1.1",
    "jest-expo": "^32.0.0",
    "moment": "^2.23.0",
    "react": "16.5.0",
    "react-apollo": "^2.4.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-navigation": "^3.0.9",
    "uuid": "^3.3.2",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-knobs": "^4.1.3",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/addon-storyshots": "^4.0.9",
    "@storybook/addons": "^4.0.9",
    "@storybook/react-native": "^4.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-preset-expo": "^5.0.0",
    "babel-runtime": "^6.26.0",
    "detox": "^9.1.2",
    "detox-expo-helpers": "^0.6.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "expo-detox-hook": "^1.0.10",
    "jest": "^24.1.0",
    "jest-fetch-mock": "^2.1.0",
    "prettier": "1.15.3",
    "prop-types": "^15.6.2",
    "react-dom": "16.5.0",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "^16.7.0"
  }
}
