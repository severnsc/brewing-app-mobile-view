{
  "name": "empty-project-template",
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-navigation-redux-helpers|@react-navigation)"
    ]
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "prestorybook": "rnstl",
    "storybook": "storybook start",
    "test": "jest --watchAll"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": ["./src/components", "./storybook"],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@storybook/addon-ondevice-knobs": "^4.1.3",
    "expo": "^31.0.2",
    "jest-expo": "^31.0.0",
    "moment": "^2.23.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-navigation": "^3.0.9",
    "uuid": "^3.3.2",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-knobs": "^4.1.3",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/addon-storyshots": "^4.0.9",
    "@storybook/addons": "^4.0.9",
    "@storybook/react-native": "^4.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-expo": "^5.0.0",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^2.1.0",
    "prettier": "1.15.3",
    "prop-types": "^15.6.2",
    "react-dom": "16.5.0",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "^16.7.0"
  }
}
