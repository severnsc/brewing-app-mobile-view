{
  "name": "empty-project-template",
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-navigation-redux-helpers|@react-navigation/.*)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "e2e"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone X, iOS 12.1"
      }
    },
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "testNamePattern": "^((?!:android:).)*$"
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "prestorybook": "rnstl",
    "storybook": "storybook start",
    "test": "jest --watchAll",
    "e2e": "detox test --configuration ios.sim",
    "e2e:dev": "detox test --configuration ios.sim --reuse"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components",
        "./storybook"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "apollo-boost": "^0.4.4",
    "apollo-link-state": "^0.4.2",
    "expo": "^35.0.0",
    "expo-constants": "^7.0.0",
    "expo-linear-gradient": "^7.0.1",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.5",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "react": "16.8.3",
    "react-apollo": "^3.1.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-navigation": "^3.11.0",
    "uuid": "^3.3.2",
    "validate.js": "^0.12.0",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-reanimated": "~1.2.0"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addon-ondevice-actions": "^5.1.3",
    "@storybook/addon-ondevice-knobs": "^5.1.3",
    "@storybook/addon-storyshots": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/react-native": "^5.1.3",
    "apollo-server": "^2.25.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-preset-expo": "^5.1.1",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.1.3",
    "detox": "^14.4.1",
    "detox-expo-helpers": "^0.6.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "expo-detox-hook": "^1.0.10",
    "jest": "^24.8.0",
    "jest-expo": "^33.0.2",
    "jest-fetch-mock": "^2.1.1",
    "prettier": "1.15.3",
    "prop-types": "^15.7.2",
    "react-dom": "16.5.0",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "^16.8.5"
  }
}
